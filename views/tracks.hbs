<table>
  <thead>
    <tr>
      <th>Song</th>
      <th>Preview</th>
    </tr>
  </thead>
  <tbody>
    {{#each tracks}}
      <tr>
        <td>
          <p>{{name}}</p>
        </td>
        <td>
          <audio controls src='{{preview_url}}'></audio>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>